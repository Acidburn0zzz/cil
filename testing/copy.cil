(block a
	(class file (read write execute_no_trans))
	(common file (open exec))
	(type foo)
	(type bar)
	(allow foo bar file (read write))
	(type blah)
	(typealias blah fooblah)
	(type blah2))

(attribute mlsfilereadtoclr)
(attribute mlsfileread)
(attribute mlstrustedobject)

(mlsconstrain (file)
	(ioctl read write create getattr setattr lock)
	(or (or (dom l1 l2)(and (== t1 mlsfilereadtoclr)(dom h1 l2)))(or (== t1 mlsfileread)(== t2 mlstrustedobject))))

(macro my_macro ((type baz)(type boz))
	(allow baz boz file (read)))

